main
{
  n = jp[0][0];
  k = jp[0][3];
  s.i = jp[0][1];
  s.j = jp[0][2];
  for(i=0;i<jp[1][0];i++){
    teleports[i].source.i=jp[i+2][0];
    teleports[i].source.j=jp[i+2][1];
    teleports[i].target.i=jp[i+2][2];
    teleports[i].target.j=jp[i+2][3];
  }
  for(i=0;i<jp[0][0];i++)for(j=0;j<jp[0][0];j++)grid[i][j]=jp[i+2+jp[1][0]][j];
}
