main
{
  n=m[0][0];
  for(i=0;i<m[0][0];i++){
    pref=m[i+1][0];
    members[i].preference=pref;
    if(pref==3 or pref==4){
      for(j=0;j<m[i+1][1];j++){
	members[i].chosen_members[j]=m[i+1][j+2];
      }
    }
  }
  for(i=0;i<m[0][1];i++){
    conflicts[i][0]=m[m[0][0]+1+i][0];
    conflicts[i][1]=m[m[0][0]+1+i][1];
  }
}
