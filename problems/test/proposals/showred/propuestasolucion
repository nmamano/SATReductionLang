reduction
{
  "a";
  not "a";
  not not "a";
  not not not "a";
  "a" or "b";
  "a"; or { "b"; };
  not "a" or "b" or not not "c";
  "a" or "b" and "c";
  "a" and "b" or "c";
  or { "a"; "b"; } or or { "c"; "d"; };
  and { "a"; "b"; } and and { "c"; "d"; };
  and { "a" and "b"; };
  exactly (1+2-3*4%5)+(6) { "a"; };
  and {};
  or {};
  atleast 0 {};
  "-a";
  "--a";
}

reconstruction
{
  if ("x{0}{0}{1}") sol[0][0]=1;
  if ("x{0}{0}{2}") sol[0][0]=2;
  if ("x{0}{1}{1}") sol[0][1]=1;
  if ("x{0}{1}{2}") sol[0][1]=2;
  if ("x{1}{0}{1}") sol[1][0]=1;
  if ("x{1}{0}{2}") sol[1][0]=2;
  if ("x{1}{1}{1}") sol[1][1]=1;
  if ("x{1}{1}{2}") sol[1][1]=2;
}