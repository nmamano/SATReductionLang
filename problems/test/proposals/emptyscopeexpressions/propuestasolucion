reduction
{
  x=and{};
  y=or{};
  x;
  not y;
  and{};
  not or{};
  "x{0}{0}{1}" or "x{0}{0}{2}";
  "x{0}{1}{1}" or "x{0}{1}{2}";
  "x{1}{0}{1}" or "x{1}{0}{2}";
  "x{1}{1}{1}" or "x{1}{1}{2}";
  not "x{0}{0}{1}" or not "x{0}{1}{1}";
  not "x{0}{0}{2}" or not "x{0}{1}{2}";
  not "x{0}{0}{1}" or not "x{1}{0}{1}";
  not "x{0}{0}{2}" or not "x{1}{0}{2}";
  not "x{1}{1}{1}" or not "x{0}{1}{1}";
  not "x{1}{1}{2}" or not "x{0}{1}{2}";
  not "x{1}{1}{1}" or not "x{1}{0}{1}";
  not "x{1}{1}{2}" or not "x{1}{0}{2}";
  if(pos[0][0]!=0)"x{0}{0}{pos[0][0]}";
  if(pos[0][1]!=0)"x{0}{1}{pos[0][1]}";
  if(pos[1][0]!=0)"x{1}{0}{pos[1][0]}";
  if(pos[1][1]!=0)"x{1}{1}{pos[1][1]}";
}

reconstruction
{
  if ("x{0}{0}{1}") sol[0][0]=1;
  if ("x{0}{0}{2}") sol[0][0]=2;
  if ("x{0}{1}{1}") sol[0][1]=1;
  if ("x{0}{1}{2}") sol[0][1]=2;
  if ("x{1}{0}{1}") sol[1][0]=1;
  if ("x{1}{0}{2}") sol[1][0]=2;
  if ("x{1}{1}{1}") sol[1][1]=1;
  if ("x{1}{1}{2}") sol[1][1]=2;
}